<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="us.easydive.playingaround">

    <application
        android:allowBackup="true"
        android:icon="@drawable/icon"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".MainActivity"
            android:label="@string/app_name"
            android:theme="@style/AppTheme.NoActionBar">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".NewStore" />
        <activity android:name=".reFill" />
        <activity android:name=".History" />
        <activity android:name=".NewValve" />
        <activity android:name=".valveHistory" />
        <activity android:name=".MessageDisplay" />
        <activity android:name=".DelStore" />
        <activity android:name=".DelValve"></activity>
    </application>

</manifest>